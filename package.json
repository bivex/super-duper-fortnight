{
  "name": "godot-smell-analyzer",
  "version": "1.0.0",
  "description": "Code smell analyzer for GDScript projects based on formal Z-specification",
  "main": "src/presentation/cli/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/presentation/cli/index.js",
    "test": "node --test test/**/*.test.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": ["godot", "gdscript", "code-smell", "analyzer", "static-analysis"],
  "author": "AI Assistant",
  "license": "MIT",
  "dependencies": {
    "tree-sitter": "^0.21.1",
    "tree-sitter-gdscript": "file:../tree-sitter-gdscript",
    "js-yaml": "^4.1.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
